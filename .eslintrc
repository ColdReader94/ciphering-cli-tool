{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2015
  },
  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off"
  }
}
